# ===========================================
# TradingAgents - 한국 시장 설정
# ===========================================

# LLM Provider (Google Gemini)
GOOGLE_API_KEY=your_gemini_api_key_here

# 기존 US 시장용 (선택사항)
ALPHA_VANTAGE_API_KEY=alpha_vantage_api_key_placeholder
OPENAI_API_KEY=openai_api_key_placeholder

# ===========================================
# 한국투자증권 OpenAPI
# https://apiportal.koreainvestment.com
# ===========================================

# 모의투자 계정 (Paper Trading)
KIS_APP_KEY_PAPER=your_paper_trading_app_key
KIS_APP_SECRET_PAPER=your_paper_trading_app_secret
KIS_ACCOUNT_PAPER=your_paper_account_number

# 실전투자 계정 (Live Trading) - 주의: 실제 돈이 거래됩니다!
KIS_APP_KEY_LIVE=your_live_trading_app_key
KIS_APP_SECRET_LIVE=your_live_trading_app_secret
KIS_ACCOUNT_LIVE=your_live_account_number

# 현재 사용 모드: "paper" 또는 "live"
KIS_MODE=paper

# ===========================================
# DART 전자공시 API (무료)
# https://opendart.fss.or.kr
# ===========================================
DART_API_KEY=your_dart_api_key_here